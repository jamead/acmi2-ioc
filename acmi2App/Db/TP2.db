#########################################################

record(longout,"$(P)EE:TP2:pulDelay-SP") {
    field(DESC, "EEPROM write")
    field(DTYP, "PSC Single I32")
    field(OUT , "@Tx 1 272")
    field(FLNK, "$(P)GoRegSingleWrite")
}

